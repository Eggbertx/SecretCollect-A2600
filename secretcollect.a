	processor 6502
	include "vcs.h"
	include "macro.h"
	SEG
	ORG $F000

Start
    CLEAN_START

FrameLoop
    VERTICAL_SYNC

	ldx #30
OverscanLoop
	sta WSYNC
	dex
	bne OverscanLoop


	jmp FrameLoop


	ORG $FFFC
	; .word Start          ; NMI, FFFA with, FFFC without
	.word Start          ; RESET
	.word Start          ; IRQ
